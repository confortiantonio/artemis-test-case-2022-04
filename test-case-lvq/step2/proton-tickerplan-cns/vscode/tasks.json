{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "cppbuild",
			"label": "C/C++: gcc build active file",
			"command": "/usr/bin/gcc",
			"args": [
				"-fdiagnostics-color=always",
				"-std=c++11",
				"-lstdc++",
				"-lqpid-proton-cpp",
				"-g",
				"${file}",
				"-o",
				"${fileDirname}/bin/x64/Debug/proton-tickerplan-cns.out"
			],
			"options": {
				"cwd": "${fileDirname}"
			},
			"problemMatcher": [
				"$gcc"
			],
			"group": "build",
			"detail": "compiler: /usr/bin/gcc"
		}
	]
}